application calculator

title "Calculator"

import mobl

screen root() {
  var n1 : Num = 2
  var n2 : Num = 2

  header("Calculator")
  group {
    inputNum(n1)
    inputNum(n2)
    item { "* " outputNum(n1 * n2) }
    item { "+ " outputNum(n1 + n2) }
    item { "- " outputNum(n1 - n2) }
    item { "/ " outputNum(n1 / n2) }
  }
}