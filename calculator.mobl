application calculator

title "Calculator"

import mobl

screen root() {
  var n1 : Number = 2
  var n2 : Number = 2

  header("Calculator")
  group {
    inputNumber(n1)
    inputNumber(n2)
    item { "* " outputNumber(n1 * n2) }
    item { "+ " outputNumber(n1 + n2) }
    item { "- " outputNumber(n1 - n2) }
    item { "/ " outputNumber(n1 / n2) }
  }
}