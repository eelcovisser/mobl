module zwetser::data

import mobl

entity Account {
  username : String
  password : String
  updates  : Collection<TwitterUpdate> (inverse: account)
}

entity TwitterUpdate {
  account    : Account (inverse: updates)
  updateId   : Int
  screenName : String
  iconUrl    : String
  text       : String
  mention    : Bool
  favorite   : Bool
  date       : DateTime
}
